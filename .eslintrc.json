{
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "ENV": true,
    "IBSheet": "readonly",
    "IB_Preset": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    //    "linebreak-style": ["error", "unix"],
//    "quotes": [
//      "error",
//      "single"
//    ],
    "semi": [
      "error",
      "never"
    ],
    "no-unused-vars": "warn"
  },
  "plugins": [
  ]
}